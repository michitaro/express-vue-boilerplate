{
  "name": "echo-backend",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "npm run build && cross-env NODE_ENV=development nodemon --inspect=5859 -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "build": "npx quicktype --lang schema ../shared/api-schema/*.ts -o build/build.json"
  },
  "dependencies": {
    "@types/cookie-session": "^2.0.36",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.10",
    "ajv": "^6.6.1",
    "cookie-session": "^2.0.0-beta.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.0",
    "morgan": "^1.9.1",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^3.1.6",
    "quicktype": "^15.0.166"
  },
  "devDependencies": {
    "nodemon": "^1.18.6"
  },
  "author": {
    "name": "michitaro"
  }
}
